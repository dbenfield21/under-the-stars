<%- include('../partials/html-head') %>
<link rel="stylesheet" href="/stylesheets/campsites/index.css" />
<%- include('../partials/nav') %>
<main>
  <h1>All Campsites</h1>
  <table id="list">
     <thead>
      <tr>
        <th>Name</th>
        <th>Location</th>
        <th>Rating</th>
       
      </tr>
    </thead>
    <tbody>
      <% Campsites.forEach(function(campsite) { %>
        <tr>
          <td><%= campsite.name %></td>
          <td><%= campsite.location %></td>
          <td><%= campsite.rating %></td>
          
          <!-- <td><a href="/campsites/<%= campsite._id %>">All Campsites</a></td> -->
        </tr>
      <% })  %>
    </tbody>
  </table>
</main>

<%- include('../partials/footer') %>